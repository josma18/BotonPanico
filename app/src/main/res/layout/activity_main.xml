<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/activitymain_btn_perfilcuida"
        android:layout_width="280dp"
        android:layout_height="60dp"
        android:onClick="onClickPerfilCiudadano"
        android:text="@string/activityperfilciudadano_tv_titulo"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/activitymain_btn_inicio"
        android:layout_width="280dp"
        android:layout_height="60dp"
        android:onClick="onClickInicio"
        android:layout_marginTop="20dp"
        android:text="@string/activini_btn_inicio"
        android:textSize="22sp"
        app:layout_constraintTop_toBottomOf="@id/activitymain_btn_perfilcuida"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>